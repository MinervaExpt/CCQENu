{
  "version":16, // turn off tunes, add flux
   // Run major variables in batch, use v16 of cuts, add closure
   // this one is for inclusives.
  "mcIn":"${CCQEMAT}/playlists/${DATALOC}_p6/${MYPLAYLIST}_MC.txt",
  "dataIn":"${CCQEMAT}/playlists/${DATALOC}_p6/${MYPLAYLIST}_Data.txt", // hack for type plot of resolution
  "prescale":999,
  "playlist":"${MYPLAYLIST}",
  "DoResolution":1,
  "MinervaModel":"${MYMODEL}", // set this as an environmental to MnvTunev1 or MnvTunev2
  "recoName":"MasterAnaDev", // change if running on MAT tuples
  "tunedmc":"untuned", // uses the tunes if available
  "closure":0, // set to 1 if you want to do closure test, otherwise 0
  "scalefileIn":"${CCQEMAT}/data/FullSample_${MYMODEL}_Q2QErecoil.root", // location of anti-nu background tuning
  "TuneCategories":["qelike","qelikenot"], // categories that have tunes
  "pdg":-14, // I'm anti-neutrino
  "cutsFile":"${CCQEMAT}/AntiNuExample/AntiNuCuts_v16.json", // describe the cuts, choose them in samples
  "varsFile":"${CCQEMAT}/AntiNuExample/Variables_v16.json", // describe the variables, choose them below
  "samplesFile":"${CCQEMAT}/AntiNuExample/AntiNuSamples_v16.json",  // describe the samples, cuts are chosen here
  "runsamples":["Inclusive"], // chose which samples to run can provide a list or chose 1 using the batch
  "outRoot":"P6_inclusive_${MYPLAYLIST}_${MYMODEL}_Inclusive", // name for output files
  "AnalyzeVariables":["Q2QE","Q0QE","pzmu","ptmu","Enu","EnuHad","EnuCCQE","ThetamuDegrees","Multiplicity","recoil","ZVertex","ApothemX","ApothemY"], // choose the 1D variables
  "Analyze2DVariables":[], // choose 2D variables, 1D variables must be chosen
  "signal":{"Inclusive":"inclusive"},// used in analysis to define signal/background, key is the sample, value is what category is signal_tag
  "background":{"Inclusive":"wrongsign"},
  "data":{"Inclusive":"data"},
  // turn on/off systematics
  "responseSystematicsVersion":"2024",
  "systematics": [
    "Flux",
    "MuonMINOS",
    "MuonResolution",
    "MINOSEfficiency",
    "Angle",
    "GENIE",
    "Flux",
    "GenieRvx1pi",
    "RPA",
    "2p2h",
    "MuonMinerva",
    "geant4",
    "response"
  ],
  "fluxUniverses": 10, // # of flux universes set low to run fast!
  "NuEConstraint": 1, // apply constraints to flux
  "Geant4NeutronCV": 1, // set true by default docdb 28556
  "Geant4Elastics": 1 // set to true by default
}
